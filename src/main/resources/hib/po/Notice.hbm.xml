<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hqgl.hib.po.Notice" table="notice" catalog="hq">
        <id name="id" type="java.lang.String">
            <column name="id" length="30" />
            <generator class="assigned" />
        </id>
        <property name="title" type="java.lang.String">
            <column name="title" length="50" not-null="true" />
        </property>
        <property name="qtime" type="java.lang.String">
            <column name="qtime" length="15" />
        </property>
        <property name="jtime" type="java.lang.String">
            <column name="jtime" length="15" />
        </property>
        <property name="content" type="java.lang.String">
            <column name="content" length="300" />
        </property>
        <property name="panduan" type="java.lang.String">
            <column name="panduan" length="1" />
        </property>
        <property name="type" type="java.lang.String">
            <column name="type" length="15" />
        </property>
        <property name="name" type="java.lang.String">
            <column name="name" length="8" not-null="true" />
        </property>
        <property name="photo" type="java.lang.String">
            <column name="photo" length="30" />
        </property>
    </class>
</hibernate-mapping>
