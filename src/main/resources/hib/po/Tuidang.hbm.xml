<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hqgl.hib.po.Tuidang" table="tuidang" catalog="hq">
        <id name="TNumber" type="java.lang.String">
            <column name="t_number" length="30" />
        </id>
        <property name="TName" type="java.lang.String">
            <column name="t_name" length="30" />
        </property>
        <property name="TSex" type="java.lang.String">
            <column name="t_sex" length="30" />
        </property>
        <property name="TDate" type="java.lang.String">
            <column name="t_date" length="30" />
        </property>
        <property name="TBeizhu" type="java.lang.String">
            <column name="t_beizhu" length="300" />
        </property>
        <property name="TPhone" type="java.lang.String">
            <column name="t_phone" length="11" />
        </property>
        <property name="TEmail" type="java.lang.String">
            <column name="t_email" length="30" />
        </property>
        <property name="TAddress" type="java.lang.String">
            <column name="t_address" length="30" />
        </property>
        <property name="TCareid" type="java.lang.String">
            <column name="t_careid" length="18" />
        </property>
    </class>
</hibernate-mapping>
