<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hqgl.hib.po.Fuzhuang" table="fuzhuang" catalog="hq">
        <id name="id" type="java.lang.String">
            <column name="id" length="30" />
        </id>
        <property name="type" type="java.lang.String">
            <column name="type" length="30" />
        </property>
        <property name="jianjie" type="java.lang.String">
            <column name="jianjie" length="300" />
        </property>
        <property name="money" type="java.lang.String">
            <column name="money" length="10" />
        </property>
        <property name="panduan" type="java.lang.String">
            <column name="panduan" length="1" />
        </property>
        <property name="lipin" type="java.lang.String">
            <column name="lipin" length="30" />
        </property>
        <property name="photo" type="java.lang.String">
            <column name="photo" length="30" />
        </property>
        <property name="zhaopian" type="java.lang.String">
            <column name="zhaopian" length="300" />
        </property>
    </class>
</hibernate-mapping>
