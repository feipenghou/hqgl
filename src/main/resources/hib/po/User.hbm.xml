<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hqgl.hib.po.User" table="user" catalog="hq">
        <id name="id" type="java.lang.String">
            <column name="id" length="30" />
        </id>
        <property name="pass" type="java.lang.String">
            <column name="pass" length="15" not-null="true" />
        </property>
        <property name="name" type="java.lang.String">
            <column name="name" length="8" not-null="true" />
        </property>
        <property name="sex" type="java.lang.String">
            <column name="sex" length="2" />
        </property>
        <property name="birthday" type="java.lang.String">
            <column name="birthday" length="20" />
        </property>
        <property name="address" type="java.lang.String">
            <column name="address" length="30" />
        </property>
        <property name="phone" type="java.lang.String">
            <column name="phone" length="12" />
        </property>
        <property name="shengfen" type="java.lang.String">
            <column name="shengfen" length="18" />
        </property>
        <property name="xueli" type="java.lang.String">
            <column name="xueli" length="8" />
        </property>
        <property name="minzu" type="java.lang.String">
            <column name="minzu" length="10" />
        </property>
        <property name="panduan" type="java.lang.String">
            <column name="panduan" length="1" />
        </property>
        <property name="email" type="java.lang.String">
            <column name="email" length="30" />
        </property>
        <property name="photo" type="java.lang.String">
            <column name="photo" length="30" />
        </property>
        <property name="qq" type="java.lang.String">
            <column name="qq" length="13" />
        </property>
        <property name="jianjie" type="java.lang.String">
            <column name="jianjie" length="300" />
        </property>
    </class>
</hibernate-mapping>
